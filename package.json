{
  "name": "web-server",
  "version": "1.0.0",
  "description": "Starter express web server template",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run copy-files",
    "clean": "rm -rf dist",
    "copy-files": "mkdir -p dist && cp -r *.js package*.json dist/",
    "package": "npm run build && cd dist && tar -czf ../web-server-${npm_package_version}.tar.gz ."
  },
  "keywords": [
    "node",
    "web",
    "server",
    "template",
    "sit774"
  ],
  "author": "SIT223 - Xinyu",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.3"
  }
}